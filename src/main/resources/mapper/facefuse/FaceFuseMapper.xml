<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anonym.module.fuseuser.FaceFuseUserDao">

    <resultMap id="faceFuseUserEntity" type="com.anonym.module.fuseuser.domain.FaceFuseUserEntity"></resultMap>

    <select id="selectByPhone" resultMap="faceFuseUserEntity">
        SELECT
            face_fuse_user_id,
            phone,
            nickname,
            PASSWORD,
            update_time,
            create_time
        FROM
            t_face_fuse_user
        WHERE
            phone = #{phone}
    </select>


</mapper>